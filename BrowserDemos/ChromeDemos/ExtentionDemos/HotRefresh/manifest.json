{
  "manifest_version": 2,

  "name": "baidu hotnews refresh",
  "description": "demo",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
	    "matches": ["https://www.baidu.com/*"],
		"js": ["content.js"]
	}
  ]
}